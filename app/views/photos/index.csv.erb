<%- headers = ['id', 'ptype', 'angle', 'state', 'source', 'description', 'camera', 'rating', 'ftype', "#{@ftype}_url", 'imageabletype', 'imageableid', 'created_at', 'updated_at'] -%>
<%= CSV.generate_line headers -%>
<%- @photos.each do | photo | -%>
  <% pfile = photo.file(@ftype) %>
  <% purl = pfile ? image_url(pfile.path) : '' %>
  <%= CSV.generate_line([
       photo.id,
       photo.ptype,
       photo.view_angle,
       photo.state,
       photo.source,
       photo.description,
       photo.camera,
       photo.rating,
       @ftype,
       purl,
       photo.imageable_type,
       photo.imageable_id,
       photo.created_at,
       photo.updated_at
        ], force_quotes: true
        ).html_safe -%>
<%- end -%>