<%- headers = ['id', 'ptype', 'angle', 'state', 'source', 'description', 'camera', 'rating', 'ftype', "#{@ftype}_url", 'specimenid', 'siteid' ] -%>
<%= CSV.generate_line headers -%>
<%- @photos.each do | photo | -%>
<%= CSV.generate_line([
       photo.id,
       photo.ptype,
       photo.view_angle,
       photo.state,
       photo.source,
       photo.description,
       photo.camera,
       photo.rating,
       @ftype,
       image_url(photo.file(@ftype).path),
       photo.imageable_type == 'Specimen' ? photo.imageable_id : '',
       photo.imageable_type == 'Site' ? photo.imageable_id : '',
        ], force_quotes: true
        ).html_safe -%>
<%- end -%>